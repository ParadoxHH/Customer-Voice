# Customer Voice Backend environment variables
DATABASE_URL=postgresql+psycopg://user:password@host:5432/customer_voice
ALLOWED_ORIGIN=https://customer-voice.pages.dev
# For multiple origins (e.g. local + production), separate with commas:
# ALLOWED_ORIGIN=http://localhost:5173,https://customer-voice.pages.dev
TOKEN_DIGEST_RUN=local-digest-token
OPENAI_API_KEY=
RESEND_API_KEY=
LOG_LEVEL=info
AUTH_TOKEN_SECRET=change-me
# Optional: set to limit who can register admins. Leave blank to grant first user admin.
ADMIN_INVITE_CODE=
# Optional overrides
# AUTH_TOKEN_TTL_SECONDS=604800
# AUTH_PASSWORD_MIN_LENGTH=8
